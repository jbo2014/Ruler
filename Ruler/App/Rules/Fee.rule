#region 税
	rule 员工
		init
			//object<User> user;
			fee=0;
			salary=0;
		if(salary<=5000)
			fee=100;
		else if(salary>5000 && salary<=8000)
			fee=200;
		else if(salary>8000 && salary<=10000)
			fee=300;
		return fee;
	end rule

	rule 领导
		init
			fee=0;
			salary=0;
		if(salary>10000 && salary<=20000)
			fee=500;
		else if(salary>20000)
			fee=1000;
		return fee;
	end rule
#endregion